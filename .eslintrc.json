{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["@typescript-eslint", "react", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "no-console": "warn",
    "prettier/prettier": "warn",
    "func-names":"off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
